arr1 = [2,3,2,4,8,5,1]


#solution with nested for loops:
#time complexity = O(n**2), space complexity = O(1)
def duplicate_finder(arr):
    for i in range(len(arr)):
        for j in range(len(arr)):
            if i==j:
                continue
            if arr[i]==arr[j]:
                return arr[i]
    return arr[i]
    
    
    
